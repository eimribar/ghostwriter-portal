# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# LLM API Keys
VITE_OPENAI_API_KEY=sk-...
VITE_ANTHROPIC_API_KEY=sk-ant-...
VITE_GOOGLE_API_KEY=AIza...

# Apify Configuration
VITE_APIFY_API_KEY=apify_api_...

# Optional: Cloudinary for media uploads
VITE_CLOUDINARY_CLOUD_NAME=your-cloud-name
VITE_CLOUDINARY_API_KEY=your-api-key
VITE_CLOUDINARY_API_SECRET=your-api-secret

# Optional: Email service (SendGrid/Resend)
VITE_EMAIL_API_KEY=your-email-api-key
VITE_EMAIL_FROM=noreply@yourcompany.com

# Feature Flags
VITE_ENABLE_AUTO_APPROVE=false
VITE_ENABLE_REAL_PUBLISH=false
VITE_ENABLE_ANALYTICS=true

# Development/Production
VITE_ENV=development